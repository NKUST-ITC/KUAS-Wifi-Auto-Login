<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#ffa8abad" />
        </shape>
    </item>
    <item
        android:right="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:top="1dp">
        <shape>
            <padding android:left="12dp"
                android:top="12dp"
                android:right="12dp"
                android:bottom="12dp" />
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#ffffffff" />
        </shape>
    </item>
</layer-list>